{
  "name": "pdfchat",
  "version": "1.0.0",
  "description": "Upload a PDF document and chat with it",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "author": "Philip Rosen",
  "license": "ISC",
  "dependencies": {
    "@pinecone-database/pinecone": "^4.1.0",
    "bullmq": "^5.34.10",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "ioredis": "^5.4.2",
    "multer": "^1.4.5-lts.1",
    "pdfjs-dist": "^4.10.38",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "supertest": "^7.0.0"
  }
}
